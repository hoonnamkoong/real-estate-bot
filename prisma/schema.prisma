generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SearchSetting {
  id        Int      @id @default(autoincrement())
  regions   String   // Comma separated regions
  type      String   // Transaction Type (e.g., A1)
  priceMax  Int?
  areaMin   Int?
  areaMax   Int?
  roomCount Int?
  results   Json?
  updatedAt DateTime @updatedAt
}

model PropertyNote {
  complexId String   @id
  note      String   // "High", "Mid", "Low"
  updatedAt DateTime @updatedAt
}
